  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false,
    "platform": {
      "php": "7.4"
    }
  },
  "type": "prestashop-module",
  "author": "cdigruttola",
  "require-dev": {
    "prestashop/php-dev-tools": "^5.0",
    "friendsofphp/php-cs-fixer": "^3.59"
  }
}
