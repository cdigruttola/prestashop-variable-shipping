{
  "name": "cdigruttola/variableshipping",
  "description": "Variable Shipping",
  "authors": [
    {
      "name": "cdigruttola",
      "email": "c.digruttola@hotmail.it"
    }
  ],
  "require": {
    "php": ">=7.4.0"
  },
  "autoload": {
    "psr-4": {
      "cdigruttola\\Module\\VariableShipping\\": "src/"
    },
    "classmap": [
      "variableshipping.php"
    ],
    "exclude-from-classmap": []
